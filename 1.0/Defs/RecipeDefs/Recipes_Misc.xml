<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	
	
	<RecipeDef>
		<defName>GR_NeutroamineFromSkin</defName>
		<label>extract neutroamine from Spidersnake shed skin</label>
		<description>Spidersnake shed skin can be ground and processed at a drug lab to extract minute amounts of neutroamine.</description>
		<jobString>Extracting neutroamine</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<workAmount>500</workAmount>
		<recipeUsers>
				<li>DrugLab</li>	  
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
				
					<thingDefs>
						<li>GR_SpidersnakeSkin</li>
					
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
			
		</ingredients>
		<fixedIngredientFilter>
		
			<thingDefs>
				<li>GR_SpidersnakeSkin</li>
				
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Neutroamine>1</Neutroamine>
		</products>
		
	</RecipeDef>
	
	<RecipeDef>
		<defName>GR_CraftBoarTuskSculpture</defName>
		<label>craft a boar tusk statuette</label>
		<description>Paragon boar tusks can be carefully carved and crafted into a small statuette. Cozy!</description>
		<jobString>Crafting boar tusk statuette</jobString>
		<workSpeedStat>SmeltingSpeed</workSpeedStat>
		<workSkill>Artistic</workSkill>
		<workSkillLearnFactor>0.3</workSkillLearnFactor>
		<effectWorking>CutStone</effectWorking>
		<soundWorking>Recipe_MakeStoneBlocks</soundWorking>

		<workAmount>1000</workAmount>
		<recipeUsers>
				<li>TableSculpting</li>	  
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
				
					<thingDefs>
						<li>GR_AlphaBoarTusks</li>
					
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
			
		</ingredients>
		<fixedIngredientFilter>
		
			<thingDefs>
				<li>GR_AlphaBoarTusks</li>
				
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<GR_BoarTuskStatuette>1</GR_BoarTuskStatuette>
		</products>
		
	</RecipeDef>
	
	  <RecipeDef>
    <defName>GR_CraftMechanoidInterfaceChip</defName>
    <label>make mechanoid interface chip</label>
    <description>Make a mechanoid interface chip. This is a very wasteful and expensive method, but useful if you can't get any mechanoid ship parts dropping from the sky.</description>
    <jobString>Making mechanoid interface chip.</jobString>
    <workSpeedStat>SmithingSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>10000</workAmount>
    <unfinishedThingDef>GR_UnfinishedMechanoidInterfaceChip</unfinishedThingDef>
	<recipeUsers>
				<li>FabricationBench</li>	  
		</recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentSpacer</li>
          </thingDefs>
        </filter>
        <count>3</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>600</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>250</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Gold</li>
          </thingDefs>
        </filter>
        <count>15</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>ComponentSpacer</li>
        <li>Steel</li>
        <li>Plasteel</li>
        <li>Gold</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <GR_MechanoidInterfaceChip>1</GR_MechanoidInterfaceChip>
    </products>
    <researchPrerequisite>AdvancedFabrication</researchPrerequisite>
    <skillRequirements>
      <Crafting>8</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
  </RecipeDef>
	
	
 


  
</Defs>