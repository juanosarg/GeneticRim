<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>More Mechanoids</li>
		</mods>

		<match Class="PatchOperationSequence">
		<operations>
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/RecipeDef[defName = "GR_ExtractGenesMechanoid"]/ingredients/li/filter/thingDefs</xpath>
				<value>
					<li>Corpse_Mech_Crawler</li>
					<li>Corpse_Mech_Skullywag</li>
					<li>Corpse_Mech_Flamebot</li>
					<li>Corpse_Mech_Mammoth</li>
					<li>Corpse_Mech_Assaulter</li>


				</value>
			</li>

			<li Class="PatchOperationAdd">
				<xpath>/Defs/RecipeDef[defName = "GR_ExtractGenesMechanoid"]/fixedIngredientFilter/thingDefs</xpath>
				<value>
					<li>Corpse_Mech_Crawler</li>
					<li>Corpse_Mech_Skullywag</li>
					<li>Corpse_Mech_Flamebot</li>
					<li>Corpse_Mech_Mammoth</li>
					<li>Corpse_Mech_Assaulter</li>
				</value>
			</li>


		</operations>
		</match>
	</Operation>


</Patch>