<Defs>



	<ThingDef ParentName="OrganicProductBase">
		<defName>GR_WeirdMilk</defName>
		<label>weird milk</label>
		<description>This strange milk has a sour, chemical taste.</description>
		<graphicData>
			<texPath>Things/Item/Resource/Milk</texPath>
			<color>(186,149,0)</color>
		</graphicData>
		<socialPropernessMatters>true</socialPropernessMatters>
		<statBases>
			<MarketValue>3.1</MarketValue>
			<Mass>0.045</Mass>
