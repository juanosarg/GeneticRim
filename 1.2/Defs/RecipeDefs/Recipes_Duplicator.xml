<?xml version="1.0" encoding="utf-8" ?>
<Defs>


	<RecipeDef>
		<defName>GR_DuplicateBear</defName>
		<label>duplicate bear genes</label>
		<description>Duplicate bear genetic material in exchange for a lot of organic pulp.</description>
		<jobString>Duplicating Genetic Material</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>500</workAmount>
		<workSkill>Intellectual</workSkill>		
		<recipeUsers>
				<li>GR_GeneDuplicator</li>	  
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						
						<li>GR_BearGenetic</li>
						
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						
						<li>GR_OrganicPulp</li>
						
					</thingDefs>
				</filter>
				<count>20</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>		
			<thingDefs>
								
						<li>GR_BearGenetic</li>						
						<li>GR_OrganicPulp</li>
					</thingDefs>
		</fixedIngredientFilter>
		<products>
			<GR_BearGenetic>2</GR_BearGenetic>
		</products>
		<skillRequirements>
			<Intellectual>5</Intellectual>
		</skillRequirements>
	</RecipeDef>
	
