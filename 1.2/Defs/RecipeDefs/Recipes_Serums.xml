<Defs>

	
	

	
	<RecipeDef>
		<defName>GR_ExtractSerumBear</defName>
		<label>extract paragon serum from a bear</label>
		<description>Extract paragon serum from a bear corpse.</description>
		<jobString>Extracting paragon serum</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>500</workAmount>
		<workSkill>Intellectual</workSkill>
		<recipeUsers>
				<li>GR_AlphaSerumExtractionTable</li>	  
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
				
					<thingDefs>
						<li>Corpse_Bear_Grizzly</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
		
			<thingDefs>
				<li>Corpse_Bear_Grizzly</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<GR_BearSerum>1</GR_BearSerum>
		</products>
		<skillRequirements>
			<Intellectual>5</Intellectual>
		</skillRequirements>
	</RecipeDef>
	
