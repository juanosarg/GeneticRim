<Defs>
	<!-- Cat - Bear -->
	<ThingDef ParentName="AnimalThingBase">
		<defName>GR_Catbear</defName>
		<label>catbear</label>
		<description>A cat / bear hybrid. Catbears are intelligent and affectionate, a perfect companion. They are also quite more resilient than your average cat.\n\nUseful abilities: Like all cat hybrids, they have an improved melee dodge ability, which in the case of these small animals is ideal to get out of a fight, and a short nuzzle period.</description>
		<statBases>
			<MoveSpeed>4</MoveSpeed>
			<MarketValue>150</MarketValue>
			<ComfyTemperatureMin>-25</ComfyTemperatureMin>
		</statBases>
		<tools>
			<li>
				<label>left claw</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>6</power>
				<cooldownTime>1.5</cooldownTime>
				<linkedBodyPartsGroup>FrontLeftPaw</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>right claw</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>6</power>
				<cooldownTime>1.5</cooldownTime>
				<linkedBodyPartsGroup>FrontRightPaw</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>15</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				<chanceFactor>0.7</chanceFactor>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>20</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>3</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
			</li>
		</tools>
		<comps>
			<li Class="DraftingPatcher.CompProperties_Draftable">
				<rage>true</rage>
				<catreflexes>true</catreflexes>
			</li>
			<li Class="NewAnimalSubproducts.CompProperties_InitialHediff">
				<hediffname>GR_MeleeDodge</hediffname>
				<hediffseverity>1</hediffseverity>
			</li>
		</comps>
		<race>
			<fleshType>GR_Normal</fleshType>
			<thinkTreeMain>GR_DraftableAnimal</thinkTreeMain>
			<body>QuadrupedAnimalWithPawsAndTail</body>
			<predator>true</predator>
			<maxPreyBodySize>0.5</maxPreyBodySize>
			<baseBodySize>0.255</baseBodySize>
			<baseHungerRate>0.1</baseHungerRate>
			<baseHealthScale>1</baseHealthScale>
			<foodType>CarnivoreAnimal, OvivoreAnimal</foodType>
			<useLeatherFrom>Cat</useLeatherFrom>
			<useMeatFrom>Cat</useMeatFrom>
			<wildness>0.36</wildness>
			<trainability>Intermediate</trainability>
			<nuzzleMtbHours>20</nuzzleMtbHours>
			<gestationPeriodDays>24</gestationPeriodDays>
			<nameOnTameChance>1</nameOnTameChance>
			<litterSizeCurve>
				<points>
					<li>(0.5, 0)</li>
					<li>(1, 1)</li>
					<li>(2, 1.7)</li>
					<li>(3, 0.7)</li>
					<li>(3.5, 0)</li>
				</points>
			</litterSizeCurve>
			<lifeExpectancy>10</lifeExpectancy>
			<lifeStageAges>
				<li>
					<def>AnimalBaby</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>0.15</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>0.4</minAge>
					<soundWounded>Pawn_Cat_Injured</soundWounded>
					<soundDeath>Pawn_Cat_Death</soundDeath>
					<soundCall>Pawn_Cat_Call</soundCall>
					<soundAngry>Pawn_Cat_Angry</soundAngry>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
		</race>
		<tradeTags>
			<li>AnimalGenetic</li>

		</tradeTags>
	</ThingDef>

