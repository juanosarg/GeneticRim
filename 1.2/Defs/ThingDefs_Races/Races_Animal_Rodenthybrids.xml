<Defs>

	<!-- Rodent - Bear -->
	<ThingDef ParentName="AnimalThingBase">
		<defName>GR_Molebear</defName>

		<label>molebear</label>
		<description>A Mole / Bear hybrid. The molebear combines the resilience of a bear with the extremely low methabolic rate of a mole rat. These creatures lack pain sensitivity, so if you want to kill one, there is no "knocking it unconscious".\n\nUseful abilities: No pain sensitivity. Fast breeding.</description>
		<statBases>
			<MoveSpeed>3</MoveSpeed>
			<ComfyTemperatureMin>-40</ComfyTemperatureMin>
			<MarketValue>275</MarketValue>
		</statBases>
		<tools>

			<li>
				<label>left claw</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>6</power>
				<cooldownTime>1.65</cooldownTime>
				<linkedBodyPartsGroup>FrontLeftPaw</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>10</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>right claw</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>6</power>
				<cooldownTime>1.65</cooldownTime>
				<linkedBodyPartsGroup>FrontRightPaw</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>10</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>12</power>
				<cooldownTime>2.1</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>15</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>3</power>
				<cooldownTime>1.65</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>      </li>
		</tools>
		<comps>
			<li Class="DraftingPatcher.CompProperties_Draftable">
				<rage>true</rage>
				<burrowing>true</burrowing>
			</li>
			<li Class="NewAnimalSubproducts.CompProperties_InitialHediff">
				<hediffname>GR_NoPain</hediffname>
				<hediffseverity>1</hediffseverity>
			</li>
		</comps>

		<race>
			<thinkTreeMain>GR_DraftableAnimal</thinkTreeMain>
			<body>QuadrupedAnimalWithPaws</body>
			<predator>true</predator>
			<fleshType>GR_Normal</fleshType>
			<baseBodySize>2</baseBodySize>
			<baseHungerRate>0.1</baseHungerRate>
			<baseHealthScale>2.5</baseHealthScale>
			<foodType>CarnivoreAnimal</foodType>
			<mateMtbHours>5</mateMtbHours>
			<gestationPeriodDays>10</gestationPeriodDays>
			<nameOnTameChance>1</nameOnTameChance>
			<trainability>Advanced</trainability>
			<wildness>0.6</wildness>
			<nuzzleMtbHours>180</nuzzleMtbHours>
			<manhunterOnDamageChance>1</manhunterOnDamageChance>
			<manhunterOnTameFailChance>0.018</manhunterOnTameFailChance>
			<useLeatherFrom>Rat</useLeatherFrom>
			<useMeatFrom>Rat</useMeatFrom>
			<litterSizeCurve>
				<points>
					<li>(0.5, 0)</li>
					<li>(1, 1)</li>
					<li>(2, 1.7)</li>
					<li>(3, 0.7)</li>
					<li>(3.5, 0)</li>
				</points>
			</litterSizeCurve>
			<lifeExpectancy>12</lifeExpectancy>
			<lifeStageAges>
				<li>
					<def>AnimalBaby</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>0.15</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>0.35</minAge>
					<soundWounded>Pawn_Bear_Injured</soundWounded>
					<soundDeath>Pawn_Bear_Death</soundDeath>
					<soundCall>Pawn_Bear_Call</soundCall>
					<soundAngry>Pawn_Bear_Angry</soundAngry>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
		</race>
		<tradeTags>
			<li>AnimalGenetic</li>
		</tradeTags>
	</ThingDef>
