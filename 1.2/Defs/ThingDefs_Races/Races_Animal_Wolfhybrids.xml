<Defs>
	<!-- Wolf - Bear -->
	<ThingDef ParentName="AnimalThingBase">
		<defName>GR_Wolfbear</defName>
		<label>wolfbear</label>
		<description>A Wolf / Bear hybrid. Very similar to the standard timber wolf, but with better stamina.\n\nUseful abilities: Good guard animal. Butchering gives Wolfbear paws, which can be sold.</description>
		<statBases>
			<MoveSpeed>5.0</MoveSpeed>
			<ComfyTemperatureMin>-50</ComfyTemperatureMin>
			<MarketValue>370</MarketValue>
		</statBases>
		<tools>

			<li>
				<label>left claw</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>11</power>
				<cooldownTime>1.65</cooldownTime>
				<linkedBodyPartsGroup>FrontLeftPaw</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>20</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>right claw</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>11</power>
				<cooldownTime>1.65</cooldownTime>
				<linkedBodyPartsGroup>FrontRightPaw</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>20</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>11</power>
				<cooldownTime>2.1</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>20</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>3</power>
				<cooldownTime>1.65</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>      </li>
		</tools>
		<comps>
			<li Class="DraftingPatcher.CompProperties_Draftable">
				<adrenalineburst>true</adrenalineburst>
				<rage>true</rage>
			</li>
		</comps>

		<race>
			<thinkTreeMain>GR_DraftableAnimal</thinkTreeMain>
			<body>QuadrupedAnimalWithPawsAndTail</body>
			<predator>true</predator>
			<fleshType>GR_Normal</fleshType>

			<baseBodySize>1</baseBodySize>
			<baseHungerRate>0.18</baseHungerRate>
			<baseHealthScale>1.5</baseHealthScale>
			<foodType>CarnivoreAnimal</foodType>
			<gestationPeriodDays>22.5</gestationPeriodDays>
			<nameOnTameChance>1</nameOnTameChance>
			<trainability>Advanced</trainability>
			<wildness>0.75</wildness>
			<nuzzleMtbHours>120</nuzzleMtbHours>
			<manhunterOnDamageChance>1</manhunterOnDamageChance>
			<manhunterOnTameFailChance>0.018</manhunterOnTameFailChance>
			<useLeatherFrom>Wolf_Timber</useLeatherFrom>
			<useMeatFrom>Wolf_Timber</useMeatFrom>
			<litterSizeCurve>
				<points>
					<li>(0.5, 0)</li>
					<li>(1, 1)</li>
					<li>(2, 1.7)</li>
					<li>(3, 0.7)</li>
					<li>(3.5, 0)</li>
				</points>
			</litterSizeCurve>
			<lifeExpectancy>14</lifeExpectancy>
			<lifeStageAges>
				<li>
					<def>AnimalBaby</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>0.2</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>0.45</minAge>
					<soundWounded>Pawn_Dog_Injured</soundWounded>
					<soundDeath>Pawn_Dog_Death</soundDeath>
					<soundCall>Pawn_Dog_Call</soundCall>
					<soundAngry>Pawn_Dog_Angry</soundAngry>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
		</race>
		<tradeTags>
			<li>AnimalGenetic</li>
		</tradeTags>
	</ThingDef>
